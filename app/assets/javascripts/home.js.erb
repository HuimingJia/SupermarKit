window.onload = function() {
  var sound = "<%= audio_path('ding.mp3') %>"
  var elevator = new Elevator({
    element: document.querySelector('.elevator-button'),
    endAudio: sound,
    mainAudio: false,
    duration: 2000
  });

  $('.elevator-button').click(function() {
    $('#user_name').focus();
  });
}
